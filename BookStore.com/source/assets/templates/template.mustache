{{#books}}<div class="tile-container col-sm-3">
<div class="tile-sub-container">
<div class="heartIcon"><i class="fa fa-heart"></i></div>
<img class="image" src="{{image}}"/>
<div class="info-box">
<div class="title"><a href="#" class="my-link" data-toggle="modal" data-target="#myModal" id="{{isbn13}}">{{title}}</a></div>
<div class="price">Price: {{price}}</div>
<div class="subtitle">{{subtitle}}</div>
<div class="isbn">ISBN13: {{isbn13}}</div>
</div></div></div>{{/books}}

<div class="modal fade" id="myModal" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal">&times;
      </button>

      <h4 class="modal-title"></h4>
      </div>
      <div class="modal-body">
      <div><img id="model-img"/></div>
      <div id="model-subtitle"></div>
      <div id="model-price"></div>
      <div id="model-isbn"></div>

      </div>
    <div class="modal-footer">
      <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
    </div>
  </div>
</div>
</div>
